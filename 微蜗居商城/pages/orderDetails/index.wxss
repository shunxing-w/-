    margin-right: 50rpx;
}
.logistics{
  border-bottom: 1px solid #F2EFF5;
}
.fr{
  float: right;
}
.total{
  display:block;
  float:right;
  width:230rpx;
  text-align:right;
}
.box1{
    border-top:5px solid #F2EFF5;
    font-size: 16px;
    overflow: hidden;
    padding:8rpx 10rpx;
    background-color: #FFFFFF;

}
.border {
  background: url(https://www.weiwoju.com/Public/wap/dinner2/image/add_border.png);
  height: 15rpx;
}
.order_total{
    /* float: right; */
    line-height: 70rpx;
    margin-left:10rpx;
}
.order_total text{
    font-size: 16px;
    margin-right: 30rpx;
}
.box1 .statye{
    margin-top:45rpx;
    float: right;
    margin-right:17rpx;
}
.box1 .st{
    margin-top:-70rpx;
   
}
.radio{
  display: block;
  border-bottom:1px solid #f2eff5;
  line-height:80rpx;
  font-size: 14px;
}
.box1 .statye text{
  text-align:center;
  height:50rpx;
  line-height:50rpx;
  margin-bottom:20rpx;
  border:1px solid #09bb07;
  padding:8rpx 12rpx;
  border-radius:3px;
  color:#09bb07;
  display:block;
  float:left;
  margin-left:25rpx;
}
.box2{
  font-size:11pt;
    margin-top:10rpx;
    padding:10rpx 25rpx;
    background-color:#FFFFFF;
}
.box2 view{
    overflow: hidden;
    height: 50rpx;
    line-height: 50rpx;
}
.box2 view text:nth-child(1){
    float: left;
    color: #b3b3b3;
}
.box2 view text:nth-child(2){
    float: right;
}
.remarks {
    padding: 0 6rpx;
    padding-left:20rpx;
    border-top: 1px solid #ccc;
    font-size: 16px;
    height: 90rpx;
    line-height: 90rpx;
    background-color: #FFFFFF;
    padding-bottom:140rpx;
}
.remarks text:nth-child(1){
    color: #b3b3b3;
    margin-right: 10rpx;
}
.added {
 width:100%;
