}
.pric{
   color: #ff6633; 
}
.shop_text{
  display: block;
  float: left;
  width:600rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.order_top{
    background-color: #FFFFFF;
    padding: 0 0.4rem;
    line-height: 70rpx;
    font-size: 14px;
  
}

.coupon_img{
  width:82%;
  height:110%;
}
.tel{
    width: 30rpx;
    height:30rpx;
